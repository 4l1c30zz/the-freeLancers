{
  "kind": "collectionType",
  "collectionName": "members",
  "info": {
    "name": "member"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Name": {
      "type": "string"
    },
    "positionZone": {
      "type": "dynamiczone",
      "components": [
        "member.position"
      ]
    },
    "About": {
      "type": "richtext"
    },
    "portfolio": {
      "collection": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "videos"
      ],
      "plugin": "upload",
      "required": false
    },
    "socialMedia": {
      "type": "component",
      "repeatable": false,
      "component": "member.social-media"
    },
    "backgroundColor": {
      "type": "string"
    },
    "shapeColor": {
      "type": "string"
    }
  }
}
